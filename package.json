{
  "name": "api-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel ./lib --out-dir ./dist --copy-files",
    "webpack-dev": "npm run webpack:clean && webpack -w --config webpack.config.js",
    "webpack": "npm run webpack:clean && webpack --config webpack.config.js",
    "webpack:clean": "rm -rf ./lib/public/dist",
    "start": "nodemon $NODE_DEBUG_OPTION ./lib/app.js --exec babel-node",
    "serve": "node ./dist/app.js",
    "test": "mocha --require babel-polyfill --require babel-register",
    "clean": "rm -rf dist",
    "eslint": "eslint . --ext .js",
    "db:create":
      "npm run build && IS_KNEX_MIGRATE=true knex --knexfile ./dist/config/database.js --cwd ./lib/config/ migrate:make",
    "db:migrate": "npm run build && IS_KNEX_MIGRATE=true knex --knexfile ./dist/config/database.js migrate:latest",
    "db:rollback": "npm run build && IS_KNEX_MIGRATE=true knex --knexfile ./dist/config/database.js migrate:rollback",
    "db:seed:create":
      "npm run build && IS_KNEX_MIGRATE=true knex --knexfile ./dist/config/database.js --cwd ./lib/config seed:make",
    "db:seed:run": "npm run build && IS_KNEX_MIGRATE=true knex --knexfile ./dist/config/database.js seed:run",
    "prettier": "prettier --write '**/*.js' '**/*json' '**/*.css' '**/*.scss'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,scss}": ["prettier --write", "git add"]
  },
  "engines": {
    "node": "==8.1.4"
  },
  "dependencies": {
    "admin-lte": "2.3.11",
    "body-parser": "~1.18.2",
    "bookshelf": "^0.11.1",
    "bookshelf-pagemaker": "^1.0.6",
    "bookshelf-secure-password": "^3.0.1",
    "bootstrap": "^3.3.7",
    "chalk": "^2.3.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "datatables.net": "^1.10.16",
    "datatables.net-bs": "^1.10.16",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "express-breadcrumbs": "0.0.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "form-builder": "0.0.1",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^3.9.0",
    "jquery": "^3.2.1",
    "knex": "^0.13.0",
    "lodash": "latest",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "pluralize": "^7.0.0",
    "select2": "^4.0.6-rc.0",
    "select2-bootstrap-theme": "0.1.0-beta.10",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "prettier": "^1.8.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
